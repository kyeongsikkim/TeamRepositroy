<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.media.*?>
<?import javafx.scene.image.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane styleClass="exitbackground" stylesheets="@exitsetting.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="securitydisplay.ExitsettingController">
   <center>
      <AnchorPane prefHeight="365.0" prefWidth="750.0" BorderPane.alignment="CENTER">
         <children>
            <HBox layoutX="206.0" layoutY="8.0" prefHeight="100.0" prefWidth="309.0">
               <children>
                  <VBox alignment="CENTER_RIGHT" prefHeight="100.0" prefWidth="179.0">
                     <children>
                        <Label fx:id="lblGetinText" text="입실지연시간" textFill="#5b5555">
                           <font>
                              <Font size="22.0" />
                           </font>
                           <VBox.margin>
                              <Insets right="5.0" />
                           </VBox.margin>
                        </Label>
                        <Label fx:id="lblGetoutText" text="퇴실지연시간" textFill="#5b5555">
                           <font>
                              <Font size="22.0" />
                           </font>
                           <VBox.margin>
                              <Insets right="5.0" />
                           </VBox.margin>
                        </Label>
                     </children>
                  </VBox>
                  <VBox alignment="CENTER_LEFT" prefHeight="100.0" prefWidth="122.0">
                     <children>
                        <HBox>
                           <children>
                              <Button id="btnHighEnter" fx:id="btnHighEnter" alignment="CENTER_RIGHT" contentDisplay="RIGHT" mnemonicParsing="false" prefWidth="50.0" styleClass="btnColor">
                                 <graphic>
                                    <ImageView fx:id="upperImage1" fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
                                    </ImageView>
                                 </graphic>
                              </Button>
                              <TextField fx:id="txtGetinTime" editable="false" prefHeight="27.0" prefWidth="72.0" text="20초" />
                              <Button id="btnLowEnter" fx:id="btnLowEnter" mnemonicParsing="false" prefWidth="50.0" styleClass="btnColor">
                                 <graphic>
                                    <ImageView fx:id="lowerImage1" fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true" />
                                 </graphic></Button>
                           </children>
                        </HBox>
                        <HBox>
                           <children>
                              <Button id="btnHighLeave" fx:id="btnHighLeave" mnemonicParsing="false" styleClass="btnColor">
                                 <graphic>
                                    <ImageView fx:id="upperImage2" fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
                                    </ImageView>
                                 </graphic>
                              </Button>
                              <TextField fx:id="txtGetoutTime" editable="false" prefHeight="27.0" prefWidth="50.0" text="60초" />
                              <Button id="btnLowLeave" fx:id="btnLowLeave" mnemonicParsing="false" styleClass="btnColor">
                                 <graphic>
                                    <ImageView fx:id="lowerImage2" fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true" />
                                 </graphic></Button>
                           </children>
                        </HBox>
                     </children>
                  </VBox>
               </children>
            </HBox>
            <Button id="btnExitSetting" fx:id="btnExitSetting" layoutX="210.0" layoutY="324.0" mnemonicParsing="false" prefWidth="300.0" text="설정" />
            <VBox alignment="CENTER" layoutX="44.0" layoutY="100.0" prefHeight="200.0" prefWidth="100.0">
               <children>
                  <ImageView fitHeight="120.0" fitWidth="120.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@image/doorway.png" />
                     </image>
                  </ImageView>
                  <CheckBox id="checkMotion" fx:id="checkMotion" mnemonicParsing="false" text="동체감지" />
               </children>
            </VBox>
            <VBox alignment="CENTER" layoutX="197.0" layoutY="100.0" prefHeight="200.0" prefWidth="100.0">
               <children>
                  <ImageView fitHeight="120.0" fitWidth="120.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@image/compter-Construction-crane-oil-gas-valve-fuel-sensor--512.png" />
                     </image>
                  </ImageView>
                  <CheckBox id="checkGas" fx:id="checkGas" mnemonicParsing="false" text="가스닫기" />
               </children>
            </VBox>
            <VBox alignment="CENTER" layoutX="375.0" layoutY="100.0" prefHeight="200.0" prefWidth="100.0">
               <children>
                  <ImageView fitHeight="120.0" fitWidth="120.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@image/geyser-512.png" />
                     </image>
                  </ImageView>
                  <CheckBox id="checkHeater" fx:id="checkHeater" mnemonicParsing="false" text="난방닫기" />
               </children>
            </VBox>
            <VBox alignment="CENTER" layoutX="553.0" layoutY="100.0" prefHeight="200.0" prefWidth="100.0">
               <children>
                  <ImageView fitHeight="120.0" fitWidth="120.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@image/flame.png" />
                     </image></ImageView>
                  <CheckBox fx:id="btnFlame" mnemonicParsing="false" text="화재/비상경보" />
               </children>
            </VBox>
         </children>
      </AnchorPane>
   </center>
   <left>
      <MediaView id="soundlock" fx:id="soundlock" BorderPane.alignment="CENTER" />
   </left>
   <right>
      <MediaView id="soundunlock" fx:id="soundunlock" BorderPane.alignment="CENTER" />
   </right>
</BorderPane>
