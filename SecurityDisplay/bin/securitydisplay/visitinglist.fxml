<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="securitydisplay.VisitinglistController">
   <children>
      <HBox alignment="TOP_CENTER" prefHeight="300.0" prefWidth="500.0" style="-fx-background-color: #ffffff;">
         <children>
            <TableView fx:id="tableView" prefHeight="200.0" prefWidth="300.0">
              <columns>
                <TableColumn prefWidth="75.0" text="방문자" />
                <TableColumn prefWidth="75.0" text="날짜" />
              </columns>
               <columnResizePolicy>
                  <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
               </columnResizePolicy>
            </TableView>
            <StackPane fx:id="imageBackground" alignment="TOP_RIGHT" prefHeight="150.0" prefWidth="200.0">
               <children>
                  <ImageView fx:id="imageView" fitHeight="300.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true" />
                  <Button fx:id="btnClose" mnemonicParsing="false" text="X" />
               </children>
            </StackPane>
         </children>
      </HBox>
   </children>
</AnchorPane>
